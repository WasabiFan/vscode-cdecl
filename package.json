{
    "name": "vscode-cdecl",
    "displayName": "vscode-cdecl",
    "description": "",
    "version": "0.0.1",
    "publisher": "kaelin",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:cdecl.explainSelected",
        "onCommand:cdecl.explainTyped"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "cdecl.explainSelected",
                "title": "cdecl: Explain selected text"
            },
            {
                "command": "cdecl.explainTyped",
                "title": "cdecl: Explain typed text"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
